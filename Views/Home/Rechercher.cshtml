@{
    ViewData["Title"] = "Rechercher";
}


<div class="col-8">


    @foreach (Jeu jeu in ViewData["resultat"] as List<Jeu>)
    {
        <article>
            <a href="jeux.html"><h5>@jeu.Nom</h5></a>
            <p>
                @jeu.Description
            </p>

        </article>
    }


</div>